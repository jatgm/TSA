[sub_resource type="GDScript" id="GDScript_kohig"]
script/source = "@tool
extends Camera2D

@onready var cat = $\"../cat\"

var shake_duration = 0.2
var shake_magnitude = 2.0
var shake_timer = 0.0

func _process(delta):
	print(position)
	if shake_timer > 0.0:
		var x = randf_range(-shake_magnitude, shake_magnitude)
		var y = randf_range(-shake_magnitude, shake_magnitude)
		position = Vector2(position.x+x, position.y+y)
		shake_timer -= delta
		
	if $\"../cat/RemoteTransform2D\".update_position == false and shake_timer < 0:
		position = cat.position

func shake():
	shake_timer = shake_duration

"

